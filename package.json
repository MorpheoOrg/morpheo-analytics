        "redux-saga": "0.15.6",
        "redux-sagas-injector": "0.2.2",
        "reselect": "3.0.1",
        "url": "0.11.0",
        "uuid": "3.1.0"
    },
    "devDependencies": {
        "atob": "^2.0.3",
        "autodll-webpack-plugin": "0.2.1",
        "autoprefixer": "7.1.2",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.0",
        "babel-eslint": "7.2.3",
