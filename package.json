{
    "name": "notebook",
    "productName": "Notebook",
    "author": "Guillaume Cisco",
    "version": "0.0.1",
    "license": "CECILL-2.1",
    "description": "Notebook frontend",
    "repository": {
        "type": "git",
        "url": "https://github.com/MorpheoOrg/morpheo-notebook"
    },
    "engines": {
        "node": ">=8.4.0",
        "npm": ">=5.3.0"
    },
    "scripts": {
        "start": "npm run clean && cross-env NODE_ENV=development babel-watch src/server/index.js",
        "start:prod": "npm run build:main && npm run serve",
        "serve": "cross-env NODE_ENV=production babel-node build/server/index.js",
        "build:main": "npm run build:client && npm run build:server && npm run build:node",
        "build:client": "rimraf build/client && cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress -p --config webpack/client.js",
        "build:server": "rimraf build/server && cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress -p --config webpack/server.js",
        "build:node": "cross-env NODE_ENV=production babel src/server/index.js -o build/server/index.js",
        "clean": "rimraf build",
        "precommit": "lint-staged --verbose",
        "lint": "eslint --fix src webpack",
        "test": "cross-env NODE_ENV=development ./node_modules/.bin/mocha --compilers js:babel-register \"./src/frontend/js/**/*.spec.js\"",
        "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-register \"./src/frontend/js/**/*.spec.js\"",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
        "eslint": "./node_modules/.bin/eslint --fix src/frontend/",
        "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
        "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register --require babel-polyfill ./src/electron/main.dev",
        "start-renderer-dev": "cross-env NODE_ENV=development babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack/electron/renderer.dev.js",
        "start:electron": "cross-env NODE_ENV=production electron ./src/electron",
        "build:electron": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
        "build-dll": "cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack/bin/webpack --progress -p --config webpack/electron/renderer.dev.dll.js --colors",
        "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress -p --config webpack/electron/main.js --colors",
        "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress -p --config webpack/electron/renderer.prod.js --colors",
        "package": "npm run build:electron && build --publish never",
        "package-all": "npm run build:electron && build -mwl",
        "package-linux": "npm run build:electron && build --linux",
        "package-win": "npm run build:electron && build --win --x64",
        "postinstall": "electron-builder install-app-deps"
    },
    "pre-commit": [
        "precommit-msg",
        "test",
        "eslint"
    ],
    "browserslist": "electron 1.6",
    "build": {
        "productName": "Notebook",
        "appId": "org.morpheo.notebook",
        "files": [
            "dist/",
            "node_modules/",
            "app.html",
            "main.prod.js",
            "main.prod.js.map",
            "package.json"
        ],
        "mac": {
            "target": "tar.gz"
        },
        "dmg": {
            "contents": [
                {
                    "x": 130,
                    "y": 220
                },
                {
                    "x": 410,
                    "y": 220,
                    "type": "link",
                    "path": "/Applications"
                }
            ]
        },
        "win": {
            "target": [
                "nsis"
            ]
        },
        "linux": {
            "target": [
                "deb",
                "AppImage"
            ]
        },
        "directories": {
            "buildResources": "resources",
            "output": "release",
            "app": "./src/electron/"
        }
    },
    "dependencies": {
        "babel-runtime": "6.26.0",
        "chai": "^4.1.2",
        "date-fns": "1.28.5",
        "emotion": "^7.2.0",
        "ends-with": "1.0.1",
        "fastclick": "1.0.6",
        "history": "4.6.3",
        "jss": "^8.1.0",
        "jss-preset-default": "^3.0.0",
        "lodash": "4.17.4",
        "material-ui": "1.0.0-beta.5",
        "material-ui-icons": "1.0.0-beta.5",
        "query-string": "5.0.0",
        "react": "15.4.2",
        "react-dom": "15.4.2",
        "react-emotion": "^7.2.0",
        "react-helmet": "5.1.3",
        "react-jss": "^7.1.0",
        "react-konami": "0.4.0",
        "react-redux": "5.0.5",
        "react-tap-event-plugin": "2.0.1",
        "react-universal-component": "2.5.0",
        "react-useragent": "0.4.4",
        "recompose": "0.24.0",
        "redux": "3.7.0",
        "redux-actions": "2.2.1",
        "redux-first-router": "1.9.15",
